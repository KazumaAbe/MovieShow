.container.user-page
  %header.user-nav
    .media
      .media-left
        = link_to image_tag(current_user.avatar, alt: '64x64', data: { holder: { rendered: true } }, class: 'media-object' ), user_path
      .media-body
        %h4#top-aligned-media.media-heading
          Top aligned media
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree #{current_user.member}
        .row
          .col-md-6
            %h4 Profile
            %p #{current_user.profile}
          .col-md-6
            %h4 Works
            %p #{current_user.works}
.container.proto-list
  .row
    / = render @prototypes.
